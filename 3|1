foot_dict= {
   'Россия': 'А' ,
   'Португалия': 'В' ,
   'Франция': 'С' ,
   'Дания': 'С' ,
   'Египет': 'А' ,
}
foot_dict['Турция']='В'


group=input()
for country in foot_dict:
   if foot_dict[country]==group:
      print(country)
values=list(set(foot_dict.values()))
for i in range(len(values)):
   if not(values[i] in values[:i]
print(values[i]+': 'str(values.country(values[i])))
