v = float(input("введите сокрость рокеты (км/с)"))
if 0 < v <= 7.8:
    print(0, "ракета упадет на землю")
elif 7.8 < v < 11.2:
    print(1, "ракета станет спутником земли")
elif 11.2 <= v <= 16.4:
    print(1, "ракета станет спутником солнца")
elif v > 16.:
    print(2, "ракета покинет солнечную систему")
else:
    print("error")
